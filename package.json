		"@types/jsonwebtoken": "^7.2.5",
		"@types/method-override": "0.0.31",
		"@types/mocha": "^5.2.0",
		"@types/morgan": "^1.7.35",
		"@types/node": "^10.1.1",
		"@types/supertest": "^2.0.4",
		"chai": "^4.1.2",
		"mocha": "^5.1.1",
		"nodemon": "^1.12.1",
		"ts-node": "^6.0.3",
		"typescript": "^2.6.2"
	}
